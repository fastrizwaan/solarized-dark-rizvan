<style-scheme name="Solarized Dark Rizvan" id="solarized-dark-rizvan" version="1.0">
  <author>Mohammed Asif Ali Rizvan</author>
  <description>Modified Solarized Dark style</description>

  <style name="text"	foreground="#93A1A1" background="#001d25" />
  <style name="selection"	foreground="#002B36" background="#657B83" />
  <style name="cursor"	foreground="#93A1A1" />
  <style name="current-line"	background="#00171d" />
  <style name="line-numbers"	foreground="#586E75" background="#001d25" />
  <style name="bracket-match"	foreground="#002B36" background="#586E75" />
  <style name="bracket-mismatch"	foreground="#DC322F" background="#586E75" />
  <style name="search-match"	foreground="#002B36" background="#B58900" />
  <style name="def:comment"	foreground="#2e3a3d" />
  <style name="def:shebang"	foreground="#586E75" bold="true" />
  <style name="def:doc-comment-element"	italic="true" />
  <style name="def:constant"	foreground="#537600" />
  <style name="def:special-char"	foreground="#859900" />
  <style name="def:identifier"	foreground="#268BD2" />
  <style name="def:statement"	foreground="#b68a00" />
  <style name="def:type"	foreground="#B58900" />
  <style name="def:operator"	foreground="#859900" />
  <style name="def:preprocessor"	foreground="#6C71C4" />
  <style name="def:error"	foreground="#DC322F" bold="true" />
  <style name="def:note"	foreground="#D33682" bold="true" />
  <style name="def:underlined"	italic="true" underline="true" />
  <style name="sh:variable-definition"	foreground="#819294" />
  <style name="sh:variable"	foreground="#4685ce" />
  <style name="sh:common-command"	foreground="#cb4b16" />
  <style name="sh:others"	foreground="#25a399" />
</style-scheme>
